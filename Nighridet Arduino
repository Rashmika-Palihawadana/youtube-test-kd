// C++ code
//
void setup()
{
 
  pinMode(1,OUTPUT);
  pinMode(2,OUTPUT);
  pinMode(3,OUTPUT);
  pinMode(4,OUTPUT);
  pinMode(5,OUTPUT);
  pinMode(6,OUTPUT);
  
}

void loop()
{
  int t=50;
  for(int i=0;i<=6;i++){
    digitalWrite(i,HIGH);
    delay(100);
    digitalWrite(i,LOW);
  }


  for(int m=5;m>1;m--){
    digitalWrite(m,HIGH);
    delay(100);
    digitalWrite(m,LOW);
  };
  
  
}
